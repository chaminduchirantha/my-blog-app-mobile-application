{
  "expo": {
    "name": "BlogHub",
    "slug": "bloghub",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myblogapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.chamindu.myblogapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9bd9c56d-e292-4fdf-ab94-3608b01f97c9"
      }
    }
  }
}